#include<stdio.h>

#define POINTER 0x10298376
#define CHANGE  16

int main()
{
    void *ptr = (void *)POINTER;
    printf("ptr is %p\n" , ptr);

    char *cPtr = (char *)ptr - CHANGE;
    printf("cPtr is %p\n" , cPtr);

    return 0;
}
